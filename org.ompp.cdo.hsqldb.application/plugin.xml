<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.ompp.cdo.hsqldb.application.Application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.ompp.cdo.hsqldb.application.Application">
         </run>
      </application>
   </extension>
   <extension
         id="Application"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.ompp.cdo.hsqldb.application.Application"
            name="OMPP HSQLDB Server">
         <property
               name="appName"
               value="OMPP HSQLDB Server">
         </property>
      </product>
   </extension>

</plugin>
